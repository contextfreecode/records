{
    "plugins": [
        [
            "@babel/plugin-syntax-record-and-tuple",
            {
                "syntaxType": "hash"
            }
        ]
    ],
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                },
                "useBuiltIns": "usage",
                "corejs": "3.6.5"
            }
        ]
    ]
}